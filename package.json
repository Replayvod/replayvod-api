  "scripts": {
    "prod": "node src/server.js",
    "start": "ts-node src/server.ts",
    "dev": "tsup-node src/server.ts --watch --on-success \"cls && node --env-file=.env dist/server.js | pino-pretty --colorize\"",
    "prebuild": "shx rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "preview": "node --env-file=.env dist/server.js",
    "db:gen": "npm run prisma generate",
    "db:studio": "npm run prisma studio",
    "db:migrate": "npm run prisma migrate dev",
    "db:push": "npm run prisma db push"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
