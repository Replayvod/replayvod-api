    "@fastify/static": "^6.11.2",
    "@prisma/client": "^5.3.0",
    "axios": "^1.4.0",
    "fastify": "^4.20.0",
    "fluent-ffmpeg": "^2.1.2",
    "moment-timezone": "^0.5.43",
    "uuid": "^9.0.0",
    "youtube-dl-exec": "^2.4.5"
  },
