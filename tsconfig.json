{
    "compilerOptions": {
      "moduleResolution": "node",
      "target": "es2017",
      "module": "commonjs",
      "outDir": "dist",
      "strict": false,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "typeRoots" : ["./node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  